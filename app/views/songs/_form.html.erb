<%= form_for(@song) do |f| %>
<div class = "field">
<%= f.label :title %>
<%= f.text_field :title %>
</div>

<div class = "field">
<%= f.label :artist %>
<%= f.text_field :artist %>
</div>

<div class = "field">
<%= f.label :comments %>
<%= f.text_field :comments %>
</div>

<div class = "field">
<%= f.label :song_url %>
<%= f.text_field :song_url %>
</div>

<div class = "actions">
<%= f.submit %>
<%end%>
</div>